package Entities;

public class Fila {
	
	public Bloco inicio;
	
	

	public void push (Bloco novo) {
		Bloco aux = this.localizar();
		aux.prox = novo;
		
	}
	
	
	public void pop() {
		inicio = inicio.prox;
	}
	
	
	private Bloco localizar() {
		Bloco aux = inicio;
		 while(aux.prox != null) {
			 aux = aux.prox;
		 }
		return aux;
		
	}
	
	

}
