package Entities;

public class PilhaFila  {
	
	public Fila fila;
	public Fila pilha;
	
	public void pilha() {
		
		Bloco aux = fila.inicio;
		pilha.inicio = fila.localizarUltimo();
		Bloco aux2 = pilha.inicio;
		
		boolean test = true;
		
		while(test) {
			while(aux.prox != aux2) {
				aux = aux.prox;
			}
			aux2.prox = aux; 
			aux = fila.inicio;
			aux2 = aux2.prox;
			
		}
		
	}
	
	
	
	
	

}
